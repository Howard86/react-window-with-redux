{
  "name": "nextjs-template",
  "version": "1.5.0",
  "description": "Opinionated Next.js template configured with TypeScript, Chakra UI, Redux Toolkit and other devtools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Howard86/nextjs-template"
  },
  "author": "Howard Tai <howard@howardism.dev>",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "chakra-ui",
    "framer-motion",
    "redux",
    "redux-toolkit",
    "rtk-query",
    "jest",
    "cypress",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint"
  ],
  "bugs": {
    "url": "https://github.com/Howard86/nextjs-template/issues"
  },
  "homepage": "https://github.com/Howard86/nextjs-template",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write .",
    "gen:theme-typings": "chakra-cli tokens src/theme/index.ts",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@ngneat/falso": "^5.3.0",
    "@reduxjs/toolkit": "^1.8.1",
    "framer-motion": "^6.3.3",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "react-window-infinite-loader": "^1.0.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.9.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@next/bundle-analyzer": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/node": "^17.0.31",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-redux": "^7.1.24",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.6",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "dayjs": "^1.11.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.4"
  }
}
